{
  "source": {
    "type": "csv",
    "file_path": "sample_data.csv",
    "delimiter": ",",
    "encoding": "utf-8"
  },
  "target": {
    "type": "s3",
    "bucket": "etl-test-bucket",
    "prefix": "data/",
    "region": "us-east-1"
  },
  "transformation": {
    "type": "merge_column",
    "config": "merge_column_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}