{
  "source": {
    "type": "excel",
    "connection": "excel_connection_string"
  },
  "target": {
    "type": "parquet",
    "connection": "parquet_connection_string"
  },
  "transformation": {
    "type": "clean",
    "operations": [
      {
        "column": "name",
        "operation": "trim"
      },
      {
        "column": "email",
        "operation": "lowercase"
      }
    ]
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}