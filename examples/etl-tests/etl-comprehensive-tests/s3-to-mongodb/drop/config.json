{
  "source": {
    "type": "s3",
    "bucket": "etl-test-bucket",
    "prefix": "data/",
    "region": "us-east-1"
  },
  "target": {
    "type": "mongodb",
    "connection_string": "mongodb://localhost:27017/",
    "database": "source_db",
    "collection": "sample_data"
  },
  "transformation": {
    "type": "drop",
    "config": "drop_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}