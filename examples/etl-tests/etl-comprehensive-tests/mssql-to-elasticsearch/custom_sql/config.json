{
  "source": {
    "type": "mssql",
    "connection": "mssql_connection_string"
  },
  "target": {
    "type": "elasticsearch",
    "connection": "elasticsearch_connection_string"
  },
  "transformation": {
    "type": "custom_sql",
    "config": "custom_sql_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}