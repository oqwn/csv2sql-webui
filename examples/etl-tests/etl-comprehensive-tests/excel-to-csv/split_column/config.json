{
  "source": {
    "type": "excel",
    "connection": "excel_connection_string"
  },
  "target": {
    "type": "csv",
    "file_path": "sample_data.csv",
    "delimiter": ",",
    "encoding": "utf-8"
  },
  "transformation": {
    "type": "split_column",
    "config": "split_column_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}