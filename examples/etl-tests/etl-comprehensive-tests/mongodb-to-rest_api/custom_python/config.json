{
  "source": {
    "type": "mongodb",
    "connection_string": "mongodb://localhost:27017/",
    "database": "source_db",
    "collection": "sample_data"
  },
  "target": {
    "type": "rest_api",
    "base_url": "https://api.example.com",
    "endpoint": "/data",
    "auth_type": "bearer",
    "pagination": "offset"
  },
  "transformation": {
    "type": "custom_python",
    "config": "custom_python_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}