{
  "categories": [
    {
      "title": "## ğŸš€ Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## ğŸ› Bug Fixes",
      "labels": ["bug", "fix"]
    },
    {
      "title": "## ğŸ§ª Tests",
      "labels": ["test", "testing"]
    },
    {
      "title": "## ğŸ“š Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## ğŸ”§ Maintenance",
      "labels": ["chore", "dependencies", "maintenance"]
    },
    {
      "title": "## ğŸ”’ Security",
      "labels": ["security"]
    }
  ],
  "ignore_labels": [
    "ignore"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{OWNER}}/${{REPOSITORY}}/compare/${{PREVIOUS_TAG}}...${{RELEASE_TAG}}",
  "pr_template": "- ${{TITLE}} by @${{AUTHOR}} in #${{NUMBER}}",
  "empty_template": "No changes in this release.",
  "label_extractor": [
    {
      "pattern": "(feat|feature)",
      "label": "feature"
    },
    {
      "pattern": "(fix|bug)",
      "label": "bug"
    },
    {
      "pattern": "(chore)",
      "label": "chore"
    }
  ],
  "duplicate_filter": {
    "pattern": "^(Merge pull request)",
    "method": "remove"
  },
  "reference": {
    "pattern": "#(\\d+)",
    "url": "${{OWNER}}/${{REPOSITORY}}/pull/$1"
  }
}