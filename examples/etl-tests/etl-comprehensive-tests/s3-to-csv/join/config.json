{
  "source": {
    "type": "s3",
    "bucket": "etl-test-bucket",
    "prefix": "data/",
    "region": "us-east-1"
  },
  "target": {
    "type": "csv",
    "file_path": "sample_data.csv",
    "delimiter": ",",
    "encoding": "utf-8"
  },
  "transformation": {
    "type": "join",
    "right_source": {
      "type": "s3",
      "table": "reference_data"
    },
    "join_type": "inner",
    "on": {
      "left": "category_id",
      "right": "id"
    }
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}