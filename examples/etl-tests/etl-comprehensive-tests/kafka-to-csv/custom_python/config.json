{
  "source": {
    "type": "kafka",
    "bootstrap_servers": "localhost:9092",
    "topics": [
      "sample_topic"
    ],
    "group_id": "etl_test_group"
  },
  "target": {
    "type": "csv",
    "file_path": "sample_data.csv",
    "delimiter": ",",
    "encoding": "utf-8"
  },
  "transformation": {
    "type": "custom_python",
    "config": "custom_python_specific_config"
  },
  "options": {
    "batch_size": 1000,
    "error_handling": "continue",
    "logging_level": "INFO"
  }
}